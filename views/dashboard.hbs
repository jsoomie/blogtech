<div class="main-container">
    <h1>
        This is the dashboard
    </h1>

    <hr>

    <div class="ul-container">
        <ul class="msg-container">
            {{#each userpost}}
            <li>
                <a href="/dashboard/details/{{id}}">
                    <h2>{{title}}</h2>
                </a>
            </li>
            <div class="vp"></div>
            {{/each}}
        </ul>
    </div>

    <div class="add-post-container">
        <button class="button" id="dashboard-addpost" onclick="window.location.href='/dashboard/add'">
            + Add New Post
        </button>
    </div>
</div>